{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "max-len": ["error", {"code": 100}],
    "indent": ["error", 2],
    "space-before-function-paren": "off"
  }
}
